          } else {
                 return PENA_INOCENCIA;
                 }
       } else {
                if (respostaPrisioneiroB == Resposta.DELACAO) {
                    return PENA_CONDENACAO_INDIVIDUAL;
                } else {
                         return PENA_CONDENACAO_CUMPLICES;
                        }
                }
     }
